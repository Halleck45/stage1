<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\CoreBundle\Entity\BetaSignup" table="beta_signup">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="email" column="email" type="string" length="255" />
        <field name="betaKey" column="beta_key" type="string" length="32" />
        <field name="createdAt" column="created_at" type="datetime" />
        <field name="updatedAt" column="updated_at" type="datetime" />
        <field name="tries" column="tries" type="integer" />
        <field name="status" column="status" type="integer" />
        <field name="emailSentAt" column="email_sent_at" type="datetime" />
</entity>

</doctrine-mapping>
