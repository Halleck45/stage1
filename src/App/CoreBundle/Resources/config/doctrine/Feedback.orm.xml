<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\CoreBundle\Entity\Feedback" table="feedback">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>

        <field name="email" column="email" type="string" length="255" nullable="true" />
        <field name="message" column="message" type="text" />
        <field name="token" column="token" type="string" length="255" />

        <many-to-one field="project" target-entity="App\CoreBundle\Entity\Project">
            <join-column name="project_id" referenced-column-name="id" />
            <cascade><cascade-remove /></cascade>
        </many-to-one>

        <many-to-one field="build" target-entity="App\CoreBundle\Entity\Build">
            <join-column name="build_id" reference-column-name="id" />
            <cascade><cascade-remove /></cascade>
        </many-to-one>

        <many-to-one field="user" target-entity="App\CoreBundle\Entity\User">
            <join-column name="user_id" referenced-column-name="id" />
            <cascade><cascade-remove /></cascade>
        </many-to-one>

</entity>

</doctrine-mapping>
